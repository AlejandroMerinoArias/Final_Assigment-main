<?xml version="1.0"?>
<package format="3">
  <name>mapping_pkg</name>
  <version>0.0.1</version>
  <description>Depth-based mapping node that builds a simple voxel occupancy map.</description>
  <maintainer email="alejandromerinoarias@gmail.com">Student</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_python</buildtool_depend>

  <depend>rclpy</depend>
  <depend>sensor_msgs</depend>
  <depend>nav_msgs</depend>
  <depend>geometry_msgs</depend>
  <exec_depend>depth_image_proc</exec_depend>
  <exec_depend>octomap_server</exec_depend>

  <exec_depend>python3-numpy</exec_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>